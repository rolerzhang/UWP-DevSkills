<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converter="using:PageUserControl.Converter">

    <converter:ListPickerItemsPickedEventArgsConverter x:Key="ListPickerItemsPickedEventArgsConverter"/>

    <Style x:Key="PaneButtonStyle" TargetType="Button">
        <Setter Property="Padding" Value="0"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
    </Style>

    <DataTemplate x:Key="SexItemTemplate">
        <TextBlock Text="{Binding}"/>
    </DataTemplate>

    <DataTemplate x:Key="ImageItemTemplate">
        <Image Width="150"
               Height="150"
               Source="{Binding}"
               Stretch="UniformToFill"/>
    </DataTemplate>


</ResourceDictionary>
